<?xml version="1.0" encoding="utf-8" ?>

<!-- Add mod extra shells to 12GaugeWithChamberAdapter -->

<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Ammo Variety - Shotguns</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/CombatExtended.AmmoSetDef[defName="SS_AmmoSet_12GaugeWithChamberAdapter"]/ammoTypes</xpath>
            <value>
                <AVCE_Ammo_12Gauge_Bird>AVCE_Bullet_12Gauge_Bird</AVCE_Ammo_12Gauge_Bird>
                <AVCE_Ammo_12Gauge_Breaching>AVCE_Bullet_12Gauge_Breaching</AVCE_Ammo_12Gauge_Breaching>
                <AVCE_Ammo_12Gauge_Dart>AVCE_Bullet_12Gauge_Dart</AVCE_Ammo_12Gauge_Dart>
				<AVCE_Ammo_12Gauge_DragonsBreath>AVCE_Bullet_12Gauge_DragonsBreath</AVCE_Ammo_12Gauge_DragonsBreath>
				<AVCE_Ammo_12Gauge_Frag>AVCE_Bullet_12Gauge_Frag</AVCE_Ammo_12Gauge_Frag>
				<AVCE_Ammo_12Gauge_Muzzlebang>AVCE_Bullet_12Gauge_Muzzlebang</AVCE_Ammo_12Gauge_Muzzlebang>
            </value>
        </match>
    </Operation>

</Patch>